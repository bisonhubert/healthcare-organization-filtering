<li class="organization_wrapper">
  <div class="organization_name">
    <%= link_to(organization.decorate.name, organization_path(organization), class: "organization_link") %>
  </div>
  <% organization.locations.each do |location| %>
    <div class="organization_location">
      <%= location.decorate.address %>
    </div>
  <% end %>
</li>
